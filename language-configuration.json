{
  "comments": {
    "lineComment": ["{% # ", "{%}"],
    "blockComment": ["{% comment %}\n", "\n{% endcomment %}"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["<", ">"],
    ["-", "-"],
    ["%", "%"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "{%", "close": "%}" },
    { "open": "{% ", "close": " %}" },
    { "open": "{%-", "close": "-%}" },
    { "open": "{%- ", "close": " -%}" },
    { "open": "{{", "close": "}}" },
    { "open": "{{ ", "close": " }}" },
    { "open": "{{-", "close": "-}}" },
    { "open": "{{- ", "close": " -}}" },
    { "open": "<", "close": ">" },
    { "open": "[", "close": "]" },
    { "open": "'", "close": "'", "notIn": ["string", "comment"] },
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "autoCloseBefore": ":.,=}])>` \n\t",
  "surroundingPairs": [
    ["-", "-"],
    ["{", "}"],
    ["%", "%"],
    ["[", "]"],
    ["(", ")"],
    ["'", "'"],
    ["\"", "\""],
    ["`", "`"]
  ]
}
